    "gatsby-plugin-netlify": "^3.7.1",
    "gatsby-plugin-offline": "^5.4.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "react": "^17.0.2",
